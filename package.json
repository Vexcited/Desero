{
  "name": "desero",
  "version": "0.3.0",
  "description": "Quickly write models to deserialize and serialize an object to another.",
  "author": "Mikkel ALMONTE--RINGAUD <contact@vexcited.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vexcited/Desero.git"
  },
  "bugs": {
    "url": "https://github.com/Vexcited/Desero/issues"
  },
  "keywords": [
    "serialization",
    "deserialization",
    "serde"
  ],
  "sideEffects": false,
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunup"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@vexcited/eslint-config": "^0.1.1",
    "bunup": "^0.16.10",
    "eslint": "^9.39.1",
    "jiti": "^2.6.1",
    "terser": "^5.44.1",
    "typescript": "^5.9.3"
  }
}
